---
layout: none
---

<?xml version="1.0" encoding="UTF-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">
	<title>Dev.Opera TV</title>
	<link href="http://dev.opera.com/"/>
	<link type="application/atom+xml" rel="self" href="http://dev.opera.com/feed/"/>
	<updated>{{ site.time | date_to_xmlschema }}</updated>
	<id>http://dev.opera.com/</id>
	<author>
		<name>Dev.Opera editors</name>
		<email></email>
	</author>

{% assign entries = site.posts | where: 'category', 'tv' | where: 'language', 'en' %}
{% for entry in entries limit:25 %}
	<entry>
		<id>http://dev.opera.com{{ entry.url }}</id>
		<link type="text/html" rel="alternate" href="http://dev.opera.com{{ entry.url }}"/>
		<title>{{ entry.title | markdownline }}</title>
		<published>{{ entry.date | date_to_xmlschema }}</published>
		<updated>{{ entry.date | date_to_xmlschema }}</updated>
		<author>
			<name>{% include author-name.html object=entry %}</name>
		</author>
		<content type="html"><![CDATA[{{ entry.content | replace: '="/','="http://dev.opera.com/' }}]]></content>
	</entry>
{% endfor %}

</feed>
