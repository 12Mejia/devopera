---
layout: nil
---
<?xml version="1.0"?>
<feed xmlns="http://www.w3.org/2005/Atom">
 
	<title>Dev.Opera</title>
	<link href="http://dev.opera.com/"/>
	<link type="application/atom+xml" rel="self" href="http://dev.opera.com/atom.xml"/>
	<updated>{{ site.time | date_to_xmlschema }}</updated>
	<id>http://dev.opera.com/</id>
	<author>
		<name>Dev.Opera editors</name>
		<email></email>
	</author>

	{% for post in site.posts %}
		{% if post.categories contains 'articles' or post.categories contains 'blog' %}
			<entry>
				<id>http://dev.opera.com{{ post.id }}</id>
				<link type="text/html" rel="alternate" href="http://dev.opera.com{{ post.url }}"/>
				<title>{{ post.title }}</title>
				<published>{{ post.date | date_to_xmlschema }}</published>
				<updated>{{ post.date | date_to_xmlschema }}</updated>
				<author>
					<name>{{ post.authors }}</name>
				</author>
				<content type="html">{{ post.content | xml_escape }}</content>
			</entry>
		{% endif %}
	{% endfor %}
 
</feed>